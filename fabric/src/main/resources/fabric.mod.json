{
    "schemaVersion": 1,
    "id": "${mod_id}",
    "version": "${version}",
    "name": "${mod_name}",
    "description": "${description}",
    "authors": [
        "${mod_author}"
    ],
    "contact": {
        "sources": "https://github.com/Potat369/Text-Formatting-Everywhere"
    },
    "license": "${license}",
    "icon": "${mod_id}.png",
    "environment": "client",
    "mixins": [
        "${mod_id}.mixins.json",
        "${mod_id}.fabric.mixins.json"
    ],
    "depends": {
        "fabricloader": "*",
        "fabric-api": "*",
        "minecraft": "*",
        "java": ">=${java_version}"
    },
    "suggests": {
        "another-mod": "*"
    }
}
  